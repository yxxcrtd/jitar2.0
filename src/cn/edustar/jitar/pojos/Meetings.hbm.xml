<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edustar.jitar.pojos.Meetings" table="S_Meetings">
		<id name="id" type="int">
			<column name="Id" />
			<generator class="identity" />
		</id>
		<property name="obj" type="java.lang.String">
			<column name="Object" length="10" not-null="true" />
		</property>
		<property name="objId" type="int">
			<column name="ObjectId" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
